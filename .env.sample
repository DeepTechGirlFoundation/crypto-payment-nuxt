# Only set IS_TEST_ENV to true if the env is Premainnet
VUE_APP_IS_PREMAIN_ENV=false
VUE_APP_USE_MAINNET=false
VUE_APP_ENABLE_ENTER_APP=true
VUE_APP_SHOW_DONATE=true
VUE_APP_ENABLE_STORE_APPS=true
